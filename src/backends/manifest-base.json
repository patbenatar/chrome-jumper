{
  "name": "Jumper",
  "description" : "Typeahead search and jump to common pages",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "omnibox": { "keyword" : "jmp" },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": ["tabs", "storage"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
